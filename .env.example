# Copia este archivo a ".env" y completa tus valores locales (NO subir a git)
AWS_REGION=us-east-1
AWS_DEFAULT_REGION=us-east-1

# Cloudflare
CLOUDFLARE_EMAIL=info@fascinantedigital.com
CLOUDFLARE_API_KEY=REEMPLAZAR

# Vercel
VERCEL_TOKEN=REEMPLAZAR
VERCEL_TEAM_ID=REEMPLAZAR

# GitHub (si algún módulo lo requiere)
GITHUB_TOKEN=REEMPLAZAR
GITHUB_OWNER=alexanderovie

# Export como TF_VAR si hace falta
TF_VAR_cloudflare_api_key=${CLOUDFLARE_API_KEY}
